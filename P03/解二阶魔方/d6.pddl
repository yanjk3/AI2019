					(when(cube7 ?x ?y ?z)
						(and
							(not(cube7 ?x ?y ?z))
							(cube5 ?x ?z ?y)
						)
					)
				)
				(forall(?x ?y ?z -color)
					(when(cube5 ?x ?y ?z)
						(and
							(not(cube5 ?x ?y ?z))
							(cube2 ?z ?x ?y)
