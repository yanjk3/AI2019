						(and
							(not(cube5 ?x ?y ?z))
							(cube2 ?x ?y ?z)
						)
					)
				)
				(forall(?x ?y ?z -color)
					(when(cube2 ?x ?y ?z)
						(and
							(not(cube2 ?x ?y ?z))
							(cube1 ?x ?y ?z)
