					(when(cube5 ?x ?y ?z)
						(and
							(not(cube5 ?x ?y ?z))
							(cube6 ?x ?y ?z)
						)
					)
				)
				(forall(?x ?y ?z -color)
					(when(cube6 ?x ?y ?z)
						(and
							(not(cube6 ?x ?y ?z))
							(cube1 ?x ?y ?z)
						)
					)
