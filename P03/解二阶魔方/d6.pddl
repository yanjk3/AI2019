(define (domain cube)
	(:requirements :strips :equality :typing)
	(:types color)
	(:predicates 
		(cube1 ?x ?y ?z -color)
		(cube2 ?x ?y ?z -color)
		(cube3 ?x ?y ?z -color)
		(cube4 ?x ?y ?z -color)
		(cube5 ?x ?y ?z -color)
		(cube6 ?x ?y ?z -color)
		(cube7 ?x ?y ?z -color)
		(cube8 ?x ?y ?z -color)
	)
	
	(:action Right
		:parameters ()
		:effect 
			(and
				(forall(?x ?y ?z -color)
					(when(cube1 ?x ?y ?z)
						(and
							(not(cube1 ?x ?y ?z))
							(cube2 ?x ?y ?z)
						)
					)
				)	
				(forall(?x ?y ?z -color)
					(when(cube2 ?x ?y ?z)
						(and
							(not(cube2 ?x ?y ?z))
							(cube5 ?x ?y ?z)
						)
					)
				)
